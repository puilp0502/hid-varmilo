#!/usr/bin/make -f
# See debhelper(7) (uncomment to enable)
# output every command that modifies files on the build system.
export DH_VERBOSE = 1

include /usr/share/dpkg/pkg-info.mk  # Provides $(DEB_VERSION_UPSTREAM)

%:
	dh $@


override_dh_auto_build:
	:

override_dh_auto_install: debian/hid-varmilo-dkms.dkms
	dh_install src/* /usr/src/hid-varmilo-$(DEB_VERSION_UPSTREAM)/
	dh_dkms -V $(DEB_VERSION_UPSTREAM)

